<?xml version="1.0" encoding="utf-8" ?>
<component name="category_screen" extends="Group">
  <script type="text/brightscript" uri="pkg:/components/screens/category_screen.brs" />
  <interface>
    <field id="category_selected" type="assocarray" alias="category_list.itemSelected" />
    <function name="updateConfig" />
  </interface>
  <children>
    <Poster
      translation="[0, 0]"
      width="1920"
      height="1080"
      uri="https://sthsroku.net/clubhub/Content/images/badminton/backgrounds/badmintonmain.jpg"
      id="category_image"
    />
    <Label
      translation="[75, 150]"
      id="env"
      font="font:LargeBoldSystemFont"
      text="Categories"
    />
    <RadioButtonList
      id="category_list"
      translation="[100, 200]"
      itemSize="[400,96]"
    />
  </children>
</component>